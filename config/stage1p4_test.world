define indicatorlight light
(
  nose "no"
  size [0.02 0.02]
  light "blink"
  matrix_render "false"
)

sub
(
   size [1.0 0.5]
   pose [0.636 0.528 0.000]
   bitmap "submarine.pnm"
   nose "no"
   color "red"
)

define swarmbot sb
(
  size [0.1 0.1]

  neighbor 1

  indicatorlight( color "red"    pose [0.030 0.000 0.000] light_interval 200) 
  indicatorlight( color "green"  pose [0.000 0.030 0.000] light_interval 400) 
  indicatorlight( color "blue"   pose [-0.030 0.000 0.000] light_interval 600) 
  indicatorlight( color "yellow" pose [0.000 -0.030 0.000] light_interval 800) 

  velocity [ 0 0 1 ]
)
	
