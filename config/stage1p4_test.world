sonarbot
(
  size [0.4 0.4]

   ranger_count 8
   ranger[0] [0.1 0.0     0.0 0.02 0.03 0.2 5.0 40.0 0.0 ]
   ranger[1] [0.1 0.1    45.0 0.02 0.03 0.2 5.0 40.0 0.0 ]
   ranger[2] [0.0 0.1    90.0 0.02 0.03 0.2 5.0 40.0 0.0 ]
   ranger[3] [-0.1 0.1  135.0 0.02 0.03 0.2 5.0 40.0 0.0 ]
   ranger[4] [-0.1 0.0  180.0 0.02 0.03 0.2 5.0 40.0 0.0 ]
   ranger[5] [-0.1 -0.1 225.0 0.02 0.03 0.2 5.0 40.0 0.0 ]
   ranger[6] [0.0 -0.1  270.0 0.02 0.03 0.2 5.0 40.0 0.0 ]
   ranger[7] [0.1 -0.1  315.0 0.02 0.03 0.2 5.0 40.0 0.0 ]

   pose [ 0.890 2.686 -1.253 ]
) 

define indicatorlight light
(
  nose "no"
  size [0.02 0.02]
  matrix_render "false"
)

sub
(
   size [1.0 0.5]
   pose [0.627 0.363 -0.003]
   bitmap "submarine.pnm"
   nose "no"
   color "red"
)

define swarmbot sb
(
  size [0.1 0.1]

  neighbor 1	

  indicatorlight( color "red"    pose [0.030 0.000 0.000] light [1 200] ) 
  indicatorlight( color "green"  pose [0.000 0.030 0.000] light [1 400]) 
  indicatorlight( color "blue"   pose [-0.030 0.000 0.000] light [1 800]) 
  indicatorlight( color "magenta" pose [0.000 -0.030 0.000] light [1 1600]) 

  velocity [ 0 0 1 ]
)
	
